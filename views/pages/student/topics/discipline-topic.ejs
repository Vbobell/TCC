<section class="page-content" data-content="viewDataTopic" data-path="topics">
    <article class="item add topic" data-item="add-topic">
            <figure>
            <h2>Novo tópico</h2>
            </figure>
            <img class="add-topic" src="images/content/itens/creativity.png"/>
            <p>Inserir novo tópico colaborativo</p>
    </article>
    <% data.disciplines.forEach(function(discipline) { %>
    <article class="item discipline topic" data-item="discipline-topic" data-id="<%= discipline.id_discipline %>">
        <figure>
        <img src=""/>
        <h2><%= discipline.name_discipline %></h2>
        </figure>
        <p>Manipule os tópicos colaborativos da disciplina</p>
    </article>
    <% }); %>
</section>